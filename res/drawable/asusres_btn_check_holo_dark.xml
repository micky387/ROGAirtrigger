<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_checked="true">
        <bitmap android:src="@drawable/asusres_btn_check_on_disabled_holo_dark" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/asusres_btn_check_off_disabled_holo_dark" />
    </item>
    <item android:state_checked="true" android:id="@id/on">
        <bitmap android:src="@drawable/asusres_btn_check_on_holo" />
    </item>
    <item android:id="@id/off">
        <bitmap android:src="@drawable/asusres_btn_check_off_holo_dark" />
    </item>
</selector>