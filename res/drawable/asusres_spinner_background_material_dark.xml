<?xml version="1.0" encoding="utf-8"?>
<inset android:insetLeft="@dimen/asusres_widget_inset_left" android:insetRight="@dimen/asusres_widget_inset_right" android:insetTop="@dimen/asusres_widget_inset_top" android:insetBottom="@dimen/asusres_widget_inset_bottom"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <selector>
        <item android:state_enabled="false">
            <layer-list android:paddingMode="stack">
                <item>
                    <nine-patch android:src="@drawable/asusres_spinner_strokes_mtrl" android:tint="@color/asusres_spinner_strokes_disabled_dark" />
                </item>
                <item android:gravity="center_vertical">
                    <nine-patch android:src="@drawable/asusres_spinner_arrow_mtrl" android:tint="?android:textColorPrimary" />
                </item>
            </layer-list>
        </item>
        <item android:state_pressed="true">
            <layer-list android:paddingMode="stack">
                <item>
                    <nine-patch android:src="@drawable/asusres_spinner_bg_mtrl" android:tint="?android:colorControlActivated" android:alpha="0.2" />
                </item>
                <item>
                    <nine-patch android:src="@drawable/asusres_spinner_strokes_mtrl" android:tint="?android:colorControlActivated" />
                </item>
                <item android:gravity="center_vertical">
                    <nine-patch android:src="@drawable/asusres_spinner_arrow_mtrl" android:tint="?android:colorControlActivated" />
                </item>
            </layer-list>
        </item>
        <item>
            <layer-list android:paddingMode="stack">
                <item>
                    <nine-patch android:src="@drawable/asusres_spinner_strokes_mtrl" android:tint="@color/asusres_spinner_strokes_normal_dark" />
                </item>
                <item android:gravity="center_vertical">
                    <nine-patch android:src="@drawable/asusres_spinner_arrow_mtrl" android:tint="?android:textColorPrimary" />
                </item>
            </layer-list>
        </item>
    </selector>
</inset>