<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="fill_horizontal|center" android:top="4.0dip">
        <shape android:shape="oval">
            <gradient android:startColor="#ff000000" android:endColor="#05000000" android:type="radial" android:gradientRadius="29.5" />
            <size android:height="@dimen/asusres_switch_thumb_size" android:width="@dimen/asusres_switch_thumb_size" />
